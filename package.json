{
  "name": "tkhquang-blog",
  "private": true,
  "scripts": {
    "fix:lib": "cp -vrR lib/* node_modules",
    "prebuild": "yarn fix:lib && chmod +x ./scripts/prebuild.sh && ./scripts/prebuild.sh",
    "add:lambda": "cd src/lambda && yarn add",
    "install:lambda": "cd src/lambda && yarn",
    "build": "yarn prebuild $HEAD && gridsome build",
    "build:lambda": "netlify-lambda build src/lambda",
    "build:test": "concurrently \"yarn dev:lambda\" \"yarn build && http-server dist --proxy http://127.0.0.1:9000\"",
    "dev:site": "gridsome develop",
    "dev:lambda": "netlify-lambda serve src/lambda",
    "dev": "concurrently \"yarn dev:lambda\" \"yarn dev:site\"",
    "explore": "gridsome explore"
  },
  "dependencies": {
    "@gridsome/plugin-google-analytics": "^0.1.1",
    "@gridsome/remark-prismjs": "^0.3.0",
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/transformer-remark": "^0.6.0",
    "@noxify/gridsome-plugin-remark-image-download": "https://github.com/noxify/gridsome-plugin-remark-image-download.git",
    "axios": "^0.19.2",
    "commentbox.io": "^2.0.0",
    "dayjs": "^1.8.28",
    "gridsome": "^0.7.17",
    "gridsome-plugin-remark-prismjs-all": "^0.3.5",
    "lodash": "^4.17.15",
    "netlify-cms": "^2.10.50",
    "prismjs": "^1.20.0",
    "slugify": "^1.4.0",
    "tailwindcss": "^1.4.6",
    "tsparticles": "^1.15.1",
    "typeface-merriweather": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "vue-icon": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-gridsome": "^1.4.12",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "http-proxy-middleware": "^1.0.4",
    "http-server": "^0.12.3",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "purgecss-whitelister": "^2.4.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.17.2"
  }
}
